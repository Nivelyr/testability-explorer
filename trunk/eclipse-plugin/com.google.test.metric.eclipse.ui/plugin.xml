<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.debug.ui.launchGroups">
      <launchGroup
            bannerImage="icons/blank.jpg"
            id="com.google.test.metric.eclipse.ui.launchGroup.testability"
            image="icons/blank.jpg"
            label="Testability"
            mode="testability">
      </launchGroup>
   </extension>


  <extension point="org.eclipse.ui.actionSets">
      <actionSet
                id="com.google.test.metric.eclipse.ui.TestabilityActionSet"
                label="Testability"
                visible="true">
             <menu
                   id="org.eclipse.ui.run"
                   label="Run"
                  path="additions">
               <separator name="relaunchGroup"/>
               <separator name="emptyLaunchGroup"/>
               </menu>
                <action
                      class="com.google.test.metric.eclipse.ui.actions.TestabilityConfigurationsAction"
                      id="com.google.test.metric.eclipse.ui.actions.testability.configurations"
                      label="Testability Configurations..."
                      menubarPath="org.eclipse.ui.run/emptyLaunchGroup">
                </action>
             <action
                   class="com.google.test.metric.eclipse.ui.actions.TestabilityHistoryAction"
                   id="com.google.test.metric.eclipse.ui.actions.testability.history"
                   label="Testability History"
                   menubarPath="org.eclipse.ui.run/emptyLaunchGroup"
                   pulldown="true"
                   state="true"
                   style="pulldown">
             </action>
</actionSet>
</extension>
  <extension
        point="org.eclipse.debug.ui.launchConfigurationTabGroups">
     <launchConfigurationTabGroup
           class="com.google.test.metric.eclipse.ui.TestabilityLaunchConfigurationTabGroup"
           id="com.google.test.metric.eclipse.ui.testabilityLaunchConfigurationTabGroup"
           type="com.google.test.metric.eclipse.core.launchTestabilityConfigurationType">
        <launchMode
              mode="testability">
        </launchMode>
     </launchConfigurationTabGroup>
  </extension>
  <extension
        point="org.eclipse.debug.ui.launchConfigurationTabs">
     <tab
           class="com.google.test.metric.eclipse.ui.TestabilityLaunchConfigurationTab"
           group="com.google.test.metric.eclipse.ui.testabilityLaunchConfigurationTabGroup"
           id="com.google.test.metric.eclipse.ui.testabilityTab"
           name="Testability">
     </tab>
  </extension>
  <extension
        point="org.eclipse.ui.views">
     <view
           class="com.google.test.metric.eclipse.ui.TestabilityReportView"
           id="com.google.test.metric.eclipse.ui.browserview"
           name="Testability Report"
           restorable="true">
     </view>
     <view
           class="com.google.test.metric.eclipse.ui.markers.TestabilityView"
           id="com.google.test.metric.eclipse.ui.testabilityView"
           name="Testability View"
           restorable="true">
     </view>
  </extension>
  <extension
        point="com.google.test.metric.eclipse.core.testabilityLaunchListener">
     <testabilityLaunchListener
           class="com.google.test.metric.eclipse.ui.internal.TestabilityReportLaunchListener">
     </testabilityLaunchListener>
  </extension>
  <extension
        point="org.eclipse.ui.editors.markerAnnotationSpecification">
     <specification
           annotationType="com.google.test.metric.ui.excellentTestability"
           colorPreferenceKey="excellentTestability_color"
           colorPreferenceValue="192,255,192"
           contributesToHeader="false"
           highlightPreferenceKey="excellentTestability_highlight"
           highlightPreferenceValue="true"
           includeOnPreferencePage="true"
           isGoToNextNavigationTarget="false"
           isGoToNextNavigationTargetKey="excellentTestability_isGoToNextNavigationTargetKey"
           isGoToPreviousNavigationTarget="false"
           isGoToPreviousNavigationTargetKey="excellentTestability_isGoToPreviousNavigationTargetKey"
           label="%excellentTestabilityAnnotation.label"
           overviewRulerPreferenceKey="excellentTestability_overviewRuler"
           overviewRulerPreferenceValue="false"
           presentationLayer="0"
           showInNextPrevDropdownToolbarAction="false"
           showInNextPrevDropdownToolbarActionKey="excellentTestability_showInNextPrevDropdownToolbarActionKey"
           textPreferenceKey="excellentTestability_text"
           textPreferenceValue="true"
           textStylePreferenceKey="excellentTestability_stylePreferences"
           textStylePreferenceValue="BOX"
           verticalRulerPreferenceKey="excellentTestability_verticalRuler"
           verticalRulerPreferenceValue="false">
     </specification>
     <specification
           annotationType="com.google.test.metric.ui.goodTestability"
           colorPreferenceKey="goodTestability_color"
           colorPreferenceValue="255,255,128"
           contributesToHeader="false"
           highlightPreferenceKey="goodTestability_highlight"
           includeOnPreferencePage="true"
           isGoToNextNavigationTargetKey="goodTestability_isGoToNextNavigationTargetKey"
           isGoToPreviousNavigationTarget="false"
           isGoToPreviousNavigationTargetKey="goodTestability_isGoToPreviousNavigationTargetKey"
           label="%goodTestabilityAnnotation.label"
           overviewRulerPreferenceKey="goodTestability_overviewRuler"
           overviewRulerPreferenceValue="false"
           presentationLayer="0"
           showInNextPrevDropdownToolbarAction="false"
           showInNextPrevDropdownToolbarActionKey="goodTestability_showInNextPrevDropdownToolbarActionKey"
           textPreferenceKey="goodTestability_text"
           textPreferenceValue="true"
           textStylePreferenceKey="goodTestability_stylePreferences"
           textStylePreferenceValue="BOX"
           verticalRulerPreferenceKey="goodTestability_verticalRuler"
           verticalRulerPreferenceValue="false">
     </specification>
     <specification
           annotationType="com.google.test.metric.ui.badTestability"
           colorPreferenceKey="badTestability_color"
           colorPreferenceValue="255,160,160"
           contributesToHeader="false"
           highlightPreferenceKey="badTestability_highlight"
           highlightPreferenceValue="true"
           includeOnPreferencePage="true"
           isGoToNextNavigationTarget="false"
           isGoToNextNavigationTargetKey="badTestability_isGoToNextNavigationTargetKey"
           isGoToPreviousNavigationTarget="false"
           isGoToPreviousNavigationTargetKey="badTestability_isGoToPreviousNavigationTargetKey"
           label="%badTestabilityAnnotation.label"
           overviewRulerPreferenceKey="badTestability_overviewRuler"
           overviewRulerPreferenceValue="false"
           presentationLayer="0"
           showInNextPrevDropdownToolbarAction="false"
           showInNextPrevDropdownToolbarActionKey="badTestability_showInNextPrevDropdownToolbarActionKey"
           textPreferenceKey="badTestability_text"
           textPreferenceValue="true"
           textStylePreferenceKey="badTestability_stylePreferences"
           textStylePreferenceValue="BOX"
           verticalRulerPreferenceKey="badTestability_verticalRuler"
           verticalRulerPreferenceValue="false">
     </specification>
  </extension>
  <extension
        id="com.google.test.metric.testabilityMarker"
        name="Testability"
        point="org.eclipse.core.resources.markers">
     <persistent
           value="true">
     </persistent>
  </extension>
  <extension
        id="teMarkerSupport"
        name="teMarkerSupport"
        point="org.eclipse.ui.ide.markerSupport">
     <markerContentGenerator
           id="com.google.test.metric.eclipse.ui.testabilityMarkerContentGenerator"
           name="testabilityMarkerContentGenerator">
        <markerFieldReference
              id="com.google.test.metric.eclipse.ui.testabilityDescriptionMarkerField"
              visible="true">
        </markerFieldReference>
        <markerFieldReference
              id="com.google.test.metric.eclipse.ui.testabilityResourceMarkerField"
              visible="true">
        </markerFieldReference>
        <markerFieldReference
              id="com.google.test.metric.eclipse.ui.testabilityPathMarkerField"
              visible="true">
        </markerFieldReference>
        <markerFieldReference
              id="com.google.test.metric.eclipse.ui.testabilityLineNumberMarkerField"
              visible="true">
        </markerFieldReference>
        <markerFieldReference
              id="com.google.test.metric.eclipse.ui.testabilityIssueTypeMarkerField"
              visible="true">
        </markerFieldReference>
        <markerTypeReference
              id="com.google.test.metric.testabilityMarker">
        </markerTypeReference>
        <markerGrouping
              id="com.google.test.metric.eclipse.ui.testabilityMarkerGrouping"
              label="Testability">
           <markerGroupingEntry
                 id="com.google.test.metric.eclipse.ui.testabilityMarkerGroupingEntry"
                 label="Type"
                 priority="1">
           </markerGroupingEntry>
           <markerAttributeGrouping
                 attribute="issueType"
                 defaultGroupingEntry="issueType"
                 markerType="com.google.test.metric.testabilityMarker">
           </markerAttributeGrouping>
        </markerGrouping>
     </markerContentGenerator>
     <markerField
           class="com.google.test.metric.eclipse.ui.markers.TestabilityDescriptionMarkerField"
           id="com.google.test.metric.eclipse.ui.testabilityDescriptionMarkerField"
           name="Description">
     </markerField>
     <markerField
           class="com.google.test.metric.eclipse.ui.markers.TestabilityResourceMarkerField"
           id="com.google.test.metric.eclipse.ui.testabilityResourceMarkerField"
           name="Resource">
     </markerField>
     <markerField
           class="com.google.test.metric.eclipse.ui.markers.TestabilityPathMarkerField"
           id="com.google.test.metric.eclipse.ui.testabilityPathMarkerField"
           name="Path">
     </markerField>
     <markerField
           class="com.google.test.metric.eclipse.ui.markers.TestabilityLineNumberMarkerField"
           id="com.google.test.metric.eclipse.ui.testabilityLineNumberMarkerField"
           name="Line Number">
     </markerField>
     <markerField
           class="com.google.test.metric.eclipse.ui.markers.TestabilityIssueTypeMarkerField"
           id="com.google.test.metric.eclipse.ui.testabilityIssueTypeMarkerField"
           name="Issue Type">
     </markerField>
  </extension>
  <extension
        point="org.eclipse.ui.ide.markerImageProviders">
     <imageprovider
           class="com.google.test.metric.eclipse.ui.markers.TestabilityMarkerImageProvider"
           id="com.google.test.metric.eclipse.ui.testabilityMarkerImageProvider"
           markertype="com.google.test.metric.testabilityMarker">
     </imageprovider>
  </extension>


</plugin>
